<launch>
  <master auto="start"/>
  <rosparam command="load" file="$(find roborts_base)/config/roborts_base_parameter.yaml" />

  <!-- Run the Base Node -->
  <node pkg="roborts_base" type="roborts_base_node" name="roborts_base_node" respawn="false" output="screen"/>

  <node pkg="roborts_camera" type="roborts_camera_node" name="roborts_camera_node" respawn="false" />
  <node pkg="roborts_detection" type="armor_detection_node" name="armor_detection_node" respawn="false" />

</launch>


